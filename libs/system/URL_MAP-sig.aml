import structure Url from "Url"

signature URL_MAP =
    sig
	type 'a map
	type 'a t = 'a map

	datatype 'a lookup_result =
	    EXISTING of 'a
	  | NEW of 'a Promise.promise

	val map: (Url.t * 'a) vector -> 'a map

	val lookup: 'a map * Url.t -> 'a option
	val lookupNew: 'a map * Url.t -> 'a lookup_result
    end
